{
    "compilerOptions": {
        "rootDir": ".",
        "outDir": "lib",
        "forceConsistentCasingInFileNames": true,
        "lib": ["ESNext", "DOM", "WebWorker", "dom.iterable"],
        "target": "ESNext",
        "module": "ESNext",
        "experimentalDecorators": true,
        "esModuleInterop": true,
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "downlevelIteration": true,
        "sourceMap": true,
        "resolveJsonModule": true,
        "moduleResolution": "Bundler",
        "strict": true,
        "strictPropertyInitialization": true,
        "noImplicitOverride": true,
        "allowJs": false,
        "checkJs": false,
        "skipLibCheck": true,
        "isolatedModules": true,
        "types": ["node"]
    },
    "include": [],
    "references": [
        {
            "path": "./packages/core"
        },
        {
            "path": "./packages/design"
        },
        {
            "path": "./packages/docs"
        },
        {
            "path": "./packages/docs-ui"
        },
        {
            "path": "./packages/engine-formula"
        },
        {
            "path": "./packages/engine-numfmt"
        },
        {
            "path": "./packages/engine-render"
        },
        {
            "path": "./packages/image"
        },
        {
            "path": "./packages/network"
        },
        {
            "path": "./packages/rpc"
        },
        {
            "path": "./packages/sheets"
        },
        {
            "path": "./packages/sheets-find"
        },
        {
            "path": "./packages/sheets-formula"
        },
        {
            "path": "./packages/sheets-import-xlsx"
        },
        {
            "path": "./packages/sheets-numfmt"
        },
        {
            "path": "./packages/sheets-ui"
        },
        {
            "path": "./packages/slides"
        },
        {
            "path": "./packages/slides-ui"
        },
        {
            "path": "./packages/ui"
        },
        {
            "path": "./packages/uniscript"
        },
        {
            "path": "./examples"
        }
    ]
}
