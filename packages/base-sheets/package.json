{
    "name": "@univerjs/base-sheets",
    "version": "0.0.1",
    "description": "UniverSheet normal base-sheets",
    "keywords": [
        "univer",
        "sheets",
        "spreadsheet"
    ],
    "author": "DreamNum <developer@univer.ai>",
    "license": "MIT",
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "publishConfig": {
        "main": "./lib/univer-base-sheets.cjs.js",
        "module": "./lib/univer-base-sheets.js",
        "types": "./lib/index.d.ts",
        "exports": {
            ".": {
                "require": "./lib/univer-base-sheets.cjs.js",
                "default": "./lib/univer-base-sheets.js"
            },
            "./*": "./lib/*",
            "./locale/*": "./lib/locale/*.js"
        },
        "access": "public"
    },
    "exports": {
        ".": "./src/index.ts",
        "./*": "./src/*",
        "./locale/*": "./src/locale/*.ts"
    },
    "directories": {
        "lib": "lib"
    },
    "files": [
        "lib"
    ],
    "private": false,
    "type": "module",
    "scripts": {
        "test": "vitest run",
        "test:watch": "vitest",
        "coverage": "vitest run --coverage",
        "esbuild": "node ./esbuild.config.mjs"
    },
    "dependencies": {
        "@ctrl/tinycolor": "^4.0.2",
        "@univerjs/base-render": "workspace:*",
        "@univerjs/core": "workspace:*",
        "@wendellhu/redi": "^0.12.10",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@types/node": "^20.8.9",
        "@vitest/coverage-istanbul": "^0.34.6",
        "esbuild": "^0.19.5",
        "happy-dom": "^12.10.1",
        "less": "^4.2.0",
        "ts-node": "^10.9.1",
        "vitest": "^0.34.6"
    }
}
