{
    "name": "@univerjs/core",
    "version": "0.0.1",
    "description": "Univer core",
    "keywords": [
        "univer",
        "core",
        "sheets",
        "docs",
        "slides",
        "excel",
        "word",
        "ppt"
    ],
    "author": "DreamNum <msoffice2@126.com>",
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "publishConfig": {
        "main": "./lib/univer-core.cjs.js",
        "module": "./lib/univer-core.js",
        "types": "./lib/index.d.ts",
        "exports": {
            ".": {
                "require": "./lib/univer-core.cjs.js",
                "default": "./lib/univer-core.js"
            },
            "./*": "./lib/*"
        },
        "access": "public"
    },
    "exports": {
        ".": "./src/index.ts",
        "./*": "./src/*"
    },
    "directories": {
        "lib": "lib"
    },
    "files": [
        "lib"
    ],
    "private": false,
    "scripts": {
        "jest": "jest --config=jest.config.js",
        "esbuild": "node ./esbuild.config.js",
        "api": "node ./api.js",
        "tsc": "tsc",
        "serve": "node test/Server/server.js"
    },
    "license": "MIT",
    "homepage": "https://github.com/dream-num/univer",
    "repository": {
        "type": "git",
        "url": "https://github.com/dream-num/univer.git"
    },
    "dependencies": {
        "@wendellhu/redi": "^0.12.7",
        "dayjs": "^1.11.5",
        "es6-proxy-polyfill": "^2.1.1",
        "nanoid": "4.0.0",
        "numeral": "^2.0.6",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@types/jest": "^29.5.4",
        "@types/node": "^18.14.0",
        "@types/react": "^18.2.21",
        "@types/react-dom": "^18.2.7",
        "jest": "29.6.4",
        "jest-dev-server": "^6.1.1",
        "less": "^4.1.3",
        "ts-jest": "29.1.1",
        "ts-node": "^10.9.1"
    }
}
