{
    "name": "@univerjs/base-render",
    "version": "0.0.1",
    "description": "UniverSheet normal base-render",
    "keywords": [
        "univer",
        "render",
        "canvas"
    ],
    "author": "DreamNum <developer@univer.ai>",
    "license": "MIT",
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "types": "./src/Types/index.d.ts",
    "publishConfig": {
        "main": "./lib/univer-base-render.cjs.js",
        "module": "./lib/univer-base-render.js",
        "types": "./lib/index.d.ts",
        "exports": {
            ".": {
                "require": "./lib/univer-base-render.cjs.js",
                "default": "./lib/univer-base-render.js"
            },
            "./*": "./lib/*"
        },
        "access": "public"
    },
    "exports": {
        ".": "./src/index.ts",
        "./*": "./src/*"
    },
    "directories": {
        "lib": "lib"
    },
    "files": [
        "lib"
    ],
    "private": false,
    "type": "module",
    "scripts": {
        "test": "vitest run",
        "test:watch": "vitest",
        "coverage": "vitest run --coverage",
        "esbuild": "node ./esbuild.config.mjs"
    },
    "dependencies": {
        "@univerjs/core": "workspace:*",
        "@wendellhu/redi": "^0.12.8",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@types/node": "^20.8.9",
        "@vitest/coverage-v8": "^0.34.6",
        "esbuild": "^0.19.5",
        "less": "^4.2.0",
        "ts-node": "^10.9.1",
        "vitest": "^0.34.6"
    }
}
