    },
    "dependencies": {
        "@univerjs/core": "workspace:*",
        "@wendellhu/redi": "^0.12.11",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@types/node": "^20.9.0",
        "@vitest/coverage-istanbul": "^0.34.6",
        "esbuild": "^0.19.5",
        "less": "^4.2.0",
        "ts-node": "^10.9.1",
        "vitest": "^0.34.6"
    }
}
