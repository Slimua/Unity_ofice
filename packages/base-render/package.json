{
    "name": "@univerjs/base-render",
    "version": "0.0.1",
    "description": "UniverSheet normal base-render",
    "keywords": [
        "univer",
        "render",
        "canvas"
    ],
    "author": "DreamNum <developer@univer.ai>",
    "license": "MIT",
    "main": "./src/index.ts",
    "publishConfig": {
        "access": "public",
        "exports": {
            ".": "./lib/index.js",
            "./*": "./lib/*"
        }
    },
    "exports": {
        ".": "./src/index.ts",
        "./*": "./src/*"
    },
    "directories": {
        "lib": "lib"
    },
    "files": [
        "lib"
    ],
