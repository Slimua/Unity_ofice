{
    "name": "@univerjs/base-ui",
    "version": "0.0.1",
    "description": "UniverSheet normal plugin UI manager",
    "keywords": [
        "univer",
        "ui"
    ],
    "author": "DreamNum <msoffice2@126.com>",
    "license": "MIT",
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "publishConfig": {
        "main": "./lib/univer-base-ui.cjs.js",
        "module": "./lib/univer-base-ui.js",
        "types": "./lib/index.d.ts",
        "exports": {
            ".": {
                "require": "./lib/univer-base-ui.cjs.js",
                "default": "./lib/univer-base-ui.js"
            },
            "./*": "./lib/*"
        },
        "access": "public"
    },
    "exports": {
        ".": "./src/index.ts",
        "./*": "./src/*"
    },
    "directories": {
        "lib": "lib"
    },
    "files": [
        "lib"
    ],
    "private": false,
    "scripts": {
        "test": "jest -c jest.config.js",
        "test:play": "npx playwright test",
        "test:debug": "PWDEBUG=1 npx playwright test",
        "test:cover": "cross-env NODE_ENV=--experimental-vm-modules jest --coverage",
        "esbuild": "node ./esbuild.config.js"
    },
    "devDependencies": {
        "@types/jest": "^27.5.2",
        "@types/node": "^18.14.0",
        "debug": "^4.3.3",
        "enzyme": "^3.11.0",
        "identity-obj-proxy": "^3.0.0",
        "jest-dev-server": "^6.1.1",
        "jest-environment-jsdom": "^29.0.3",
        "less": "^4.1.1",
        "ts-jest": "29.0.1",
        "ts-node": "^10.9.1"
    },
    "dependencies": {
        "@univerjs/base-render": "workspace:^0.0.1",
        "@univerjs/core": "workspace:^0.0.1",
        "@wendellhu/redi": "^0.12.7",
        "react-colorful": "^5.3.0",
        "rxjs": "^7.8.1"
    }
}