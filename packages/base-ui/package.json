{
    "name": "@univerjs/base-ui",
    "version": "0.0.1",
    "description": "UniverSheet normal plugin UI manager",
    "keywords": [],
    "author": "DreamNum <developer@univer.ai>",
    "license": "MIT",
    "main": "./src/index.ts",
    "types": "./lib/index.d.ts",
    "publishConfig": {
        "access": "public",
        "main": "./lib/index.cjs.js",
        "exports": {
            ".": {
                "import": "./lib/esm/index.js",
                "require": "./lib/cjs/index.js",
                "types": "./lib/types/index.d.ts"
            },
            "./*": {
                "import": "./lib/esm/*",
                "require": "./lib/cjs/*"
            },
            "./locale/*": {
                "import": "./lib/esm/locale/*.js",
                "require": "./lib/cjs/locale/*.js"
            }
        }
    },
    "exports": {
        ".": "./src/index.ts",
        "./*": "./src/*",
        "./locale/*": "./src/locale/*.ts"
    },
    "directories": {
        "lib": "lib"
    },
    "files": [
        "lib"
    ],
    "private": false,
    "type": "module",
    "scripts": {
        "test": "vitest run",
        "test:watch": "vitest",
        "coverage": "vitest run --coverage",
        "esbuild": "node ./esbuild.config.mjs && tsc -p tsconfig.esbuild.json"
    },
    "dependencies": {
        "@univerjs/base-render": "workspace:*",
        "@univerjs/core": "workspace:*",
        "@univerjs/design": "workspace:*",
        "@univerjs/icons": "^0.1.12",
        "@wendellhu/redi": "^0.12.11",
        "clsx": "^2.0.0",
        "localforage": "^1.10.0",
        "rc-notification": "^5.3.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-transition-group": "^4.4.5",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@testing-library/react": "^14.1.0",
        "@types/node": "^20.9.0",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@types/react-transition-group": "^4.4.9",
        "@vitejs/plugin-react": "^4.1.1",
        "@vitest/coverage-istanbul": "^0.34.6",
        "esbuild": "^0.19.5",
        "happy-dom": "^12.10.3",
        "less": "^4.2.0",
        "ts-node": "^10.9.1",
        "vitest": "^0.34.6"
    }
}
