{
    "name": "@univerjs/design",
    "version": "0.0.1",
    "description": "UI component library for building exceptional Univer.",
    "keywords": [
        "UI",
        "Component Library",
        "React"
    ],
    "author": "DreamNum <developer@univer.ai>",
    "license": "MIT",
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "publishConfig": {
        "main": "./lib/univer-design.cjs.js",
        "module": "./lib/univer-design.js",
        "types": "./lib/index.d.ts",
        "exports": {
            ".": {
                "require": "./lib/univer-design.cjs.js",
                "default": "./lib/univer-design.js"
            },
            "./*": "./lib/*",
            "./locale/*": "./lib/locale/*.ts"
        },
        "access": "public"
    },
    "exports": {
        ".": "./src/index.ts",
        "./*": "./src/*",
        "./locale/*": "./src/locale/*.ts"
    },
    "directories": {
        "lib": "lib"
    },
    "files": [
        "lib"
    ],
    "private": false,
    "type": "module",
    "scripts": {
        "test": "vitest run",
        "test:watch": "vitest",
        "coverage": "vitest run --coverage",
        "esbuild": "node ./esbuild.config.mjs"
    },
    "dependencies": {
        "@rc-component/color-picker": "^1.4.1",
        "@rc-component/trigger": "^1.18.1",
        "@univerjs/icons": "^0.1.8",
        "clsx": "^2.0.0",
        "rc-dialog": "^9.3.4",
        "rc-dropdown": "^4.1.0",
        "rc-input": "^1.3.6",
        "rc-input-number": "^8.5.0",
        "rc-menu": "^9.12.2",
        "rc-select": "^14.10.0",
        "rc-tooltip": "^6.1.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-draggable": "^4.4.6",
        "react-transition-group": "^4.4.5"
    },
    "devDependencies": {
        "@testing-library/react": "^14.1.0",
        "@types/node": "^20.9.0",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@types/react-transition-group": "^4.4.9",
        "@vitest/coverage-istanbul": "^0.34.6",
        "esbuild": "^0.19.5",
        "happy-dom": "^12.10.3",
        "less": "^4.2.0",
        "ts-node": "^10.9.1",
        "vitest": "^0.34.6"
    }
}
