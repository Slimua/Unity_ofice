{
    "name": "univer",
    "type": "module",
    "version": "0.1.0-beta.4",
    "private": true,
    "packageManager": "pnpm@8.6.2",
    "author": "DreamNum Inc. <developer@univer.ai>",
    "license": "Apache-2.0",
    "engines": {
        "node": ">=18.0.0"
    },
    "scripts": {
        "prepare": "husky install",
        "pre-commit": "lint-staged",
        "dev:demo": "turbo dev:demo",
        "dev:storybook": "storybook dev -p 6006 --no-open",
        "lint:types": "turbo lint:types",
        "test": "turbo test -- --passWithNoTests",
        "coverage": "turbo coverage -- --passWithNoTests",
        "build": "turbo build",
        "build:demo": "turbo build:demo",
        "build:storybook": "storybook build",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "release": "release-it",
        "release:alpha": "release-it preminor --preRelease=alpha",
        "release:beta": "release-it preminor --preRelease=beta",
        "release:rc": "release-it prerelease --preRelease=rc"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^2.6.4",
        "@commitlint/cli": "^18.6.0",
        "@commitlint/config-conventional": "^18.6.0",
        "@playwright/test": "^1.41.2",
        "@release-it-plugins/workspaces": "^4.2.0",
        "@release-it/conventional-changelog": "^8.0.1",
        "@storybook/addon-docs": "^7.6.10",
        "@storybook/addon-essentials": "^7.6.10",
        "@storybook/addon-interactions": "^7.6.10",
        "@storybook/addon-links": "^7.6.10",
        "@storybook/blocks": "^7.6.10",
        "@storybook/react": "^7.6.10",
        "@storybook/react-vite": "^7.6.10",
        "@storybook/testing-library": "^0.2.2",
        "@types/node": "^20.11.16",
        "@types/react": "^18.2.53",
        "@typescript-eslint/parser": "^6.20.0",
        "@univerjs/design": "workspace:*",
        "@vitejs/plugin-react": "^4.2.1",
        "eslint": "^8.56.0",
        "eslint-plugin-format": "^0.1.0",
        "eslint-plugin-header": "^3.1.1",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "husky": "^9.0.10",
        "lint-staged": "^15.2.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "release-it": "^17.1.1",
        "storybook": "^7.6.10",
        "turbo": "^1.12.2",
        "typescript": "^5.3.3"
    },
    "lint-staged": {
        "*": "eslint --fix"
    }
}
